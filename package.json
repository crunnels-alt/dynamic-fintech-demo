{
  "name": "dynamic-fintech-demo",
  "version": "1.0.0",
  "description": "Interactive fintech demo for Dev Days NYC showcasing Infobip Voice API with AI-powered customer service",
  "main": "src/app.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "start:proxy": "node proxy-server.js",
    "dev:proxy": "nodemon proxy-server.js",
    "start:all": "concurrently \"npm run start\" \"npm run start:proxy\"",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:proxy\"",
    "start:production": "npm run migrate:up || echo 'Migration failed, continuing with server start' && npm run start",
    "migrate:up": "node scripts/migrate-database.js",
    "validate-config": "node scripts/validate-config.js",
    "setup-db": "node scripts/setupDatabase.js",
    "test-phones": "node scripts/testPhoneNumbers.js",
    "setup-ngrok": "./scripts/setup-ngrok.sh",
    "test-offline": "node scripts/offline-test.js",
    "test-postgres": "node scripts/test-postgres.js",
    "test": "jest"
  },
  "keywords": [
    "fintech",
    "voice-ai",
    "infobip",
    "openai",
    "demo",
    "conference"
  ],
  "author": "DevRel Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.12.2",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-validator": "^7.0.1",
    "node-fetch": "^2.7.0",
    "pg": "^8.11.3",
    "sqlite3": "^5.1.7",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "jest": "^30.1.3",
    "nodemon": "^3.1.10"
  }
}
